interface I1{

// in  inerface abstract method automatically take public and abstract , we dont need to apply 

void show();
// we can create default concrete method in interface 

 default void display(){

} 


int a=10; 
//in interface all variable automatically public static final 

}

interface I2{
void name();
}

class Int implements  I1,I2 

{

// we have to implement all method of interface as public beacuse they are automatically public declared abstract.
 public void show(){
 System.out.println("I1");
}

public void name() {


System.out.println("MY name is abhishek kumar ");


}

 public static void main(String[] args) {
 
// WE cant create object of interface I1 so we have to create object of main class Int 

 Int ob1=new Int ();

 ob1.show();
 ob1.name();
}

}
