
class A{
    static void m1(){
        System.out.println("parent class A ");
    }
}
class b extends A{
   static void m2(){
        System.out.println("child class B");
     }
}
public class Inheritencee {
    public static void main(String[] args) {
        A ob1=new A();
        b ob2=new b();
        b.m1();  //here b can access the both method becuase it is a method in child class which have all property of parent class A
        b.m2();   
        A.m1();  //this object  can acceess only its own method if we try to access the child class method  by A it will be through out a error.
    }
    }
    
